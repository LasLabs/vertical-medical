<odoo>

 <record id="medical_patient_dob_view" model="ir.ui.view">
        <field name="name">medical.patient.dob.view</field>
        <field name="model">medical.patient</field>
        <field name="inherit_id" ref="medical.medical_patient_view_search" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='birthdate_date']" position="replace">
                <field name="birthdate" />
            </xpath>
        </field>
    </record>

</odoo>
